$bog_bzrl_app $mol_page
	title @ \Bzrl App
	plugins /
		<= Theme $bog_theme_auto
			theme_light \$mol_theme_light
			theme_dark \$mol_theme_dark
	tools /
		<= Theme_toggle $bog_theme_toggle
			theme_auto <= Theme
		<= Profile $bog_bzrl_app_profile
	body /
		<= Content $mol_view
			sub /
				<= Welcome $mol_paragraph
					title @ \Welcome to Bzrl!
